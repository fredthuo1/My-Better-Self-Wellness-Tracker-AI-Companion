{
  "expo": {
    "name": "My Better Self",
    "slug": "my-better-self-wellness-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mybetterself",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mybetterself.wellness"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#14b8a6"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-dev-client",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#14b8a6",
          "sounds": [
            "./assets/sounds/notification.wav"
          ],
          "mode": "production"
        }
      ],
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "build": {
          "experimental": {
            "ios": {
              "appExtensions": [
                {
                  "targetName": "OneSignalNotificationServiceExtension",
                  "bundleIdentifier": "com.mybetterself.wellness.OneSignalNotificationServiceExtension",
                  "entitlements": {
                    "com.apple.security.application-groups": [
                      "group.com.mybetterself.wellness.onesignal"
                    ]
                  }
                }
              ]
            }
          }
        },
        "projectId": "66b4837f-8d4f-49fa-8fb8-88706120d65e"
      }
    }
  }
}
